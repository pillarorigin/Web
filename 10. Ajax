$.ajax

$("#id").click(function(){
    $.ajax({
        type:"POST",
        url:"/api/list",
        data : {name : "valueê°’"},
        dataType : "data",
        success: function(data){
            console.log(data);
        },
        error: function(xhr, status, error) {
            alert(error);
        }   
    });
});

